{
  "name": "fundamental-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve src/index.ts",
    "build": "vue-cli-service build --mode production --target lib --report --name FundamentalVue src/index.ts",
    "test": "vue-cli-service test:unit --runInBand --verbose",
    "lint": "vue-cli-service lint --no-fix --max-warnings 0 --max-errors 0",
    "lint:dev": "vue-cli-service lint",
    "test:watch": "yarn test --watch"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "main": "dist/FundamentalVue.common",
  "browser": "dist/FundamentalVue.umd.min",
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-typescript": "^3.4.0",
    "@vue/cli-plugin-unit-jest": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "chai": "^4.2.0",
    "eslint": "^5.15.0",
    "eslint-plugin-vue": "^5.2.2",
    "typescript": "^3.3.3",
    "vue": "^2.6.7",
    "ts-jest": "^23.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "vue-template-compiler": "^2.6.7"
  },
  "peerDependencies": {
    "vue": "^2.6.7",
    "fiori-fundamentals": "^1.4.3"
  },
  "dependencies": {
    "focus-trap": "^4.0.2"
  }
}